{
    "name": "laraviet/backpackui1",
    "description": "Style CRUD of Backpack Generator UI",
    "license": "MIT",
    "authors": [
        {
            "name": "Nguyen Trung Thanh",
            "email": "thanhcttsp@gmail.com",
            "homepage": "http://portalbeanzvn.com"
        }
    ],
    "homepage": "https://github.com/laraviet/backpackui1",
    "keywords": ["Laravel", "BackpackUI1"],
    "require": {
        "illuminate/support": "~5",
        "backpack/crud": "^3.5"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3.0",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Laraviet\\BackpackUI1\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Laraviet\\BackpackUI1\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Laraviet\\BackpackUI1\\BackpackUI1ServiceProvider"
            ],
            "aliases": {
                "BackpackUI1": "Laraviet\\BackpackUI1\\Facades\\BackpackUI1"
            }
        }
    }
}
